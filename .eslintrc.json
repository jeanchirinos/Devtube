{
  "extends": [
    "next/core-web-vitals",
    "standard",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "no-empty": "warn",
    "no-unreachable": "warn",
    "spaced-comment": "off"
  },
  "globals": {
    "React": "readonly",
    "JSX": "readonly"
  }
}